{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/open-sans": "^4.5.2",
    "@fontsource/raleway": "^4.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "framer-motion": "^5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fetching-library": "^1.7.6",
    "react-hook-form": "^7.24.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "5.9.5",
    "@welldone-software/why-did-you-render": "6.0.5",
    "auto-changelog": "2.2.1",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-testing-library": "3.10.1",
    "full-icu": "1.3.1",
    "http-server": "0.12.3",
    "husky": "5.0.9",
    "inquirer-directory": "2.2.0",
    "lint-staged": "10.5.4",
    "miragejs": "0.1.41",
    "node-fetch": "2.6.1",
    "npm-run-all": "4.1.5",
    "plop": "2.7.4",
    "prettier": "2.2.1",
    "stylelint": "13.10.0",
    "stylelint-config-recommended": "3.0.0",
    "wait-on": "5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" && stylelint \"./src/**/*.{css,pcss,scss}\" && echo \"lint success\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\" && stylelint --fix \"./src/**/*.{css,pcss,scss}\" && echo \"lint success\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "auto-changelog": {
    "issueUrl": "https://headstart.atlassian.net/browse/{id}",
    "issuePattern": "[A-Z]+-[1-9]+"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,pcss,scss}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ]
  }
}
